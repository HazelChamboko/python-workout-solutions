    "class Scorelist():\n",
    "    def __init__(self, score):\n",
    "        self.score = score\n",
    "    \n",
    "    def average(self):\n",
    "        return sum(self.score) / len(self.score)\n",
    "    \n",
    "score = Scorelist([85,80,90,95,98])    \n",
    "print (f'The finall score is {score.average()}')"
